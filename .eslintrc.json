{
    "env": {
        "browser": true,
        "es2021": true,
        "cypress/globals": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:cypress/recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "@typescript-eslint",
        "cypress"
    ],
    "rules": {
      // Add your custom rules here
        "no-assigning-return-values": "warn",
        "no-async-tests": "warn",
        "no-unnecessary-waiting": "warn",
        "unsafe-to-chain-command": "warn",
        "prettier/prettier": "error"
    }
}